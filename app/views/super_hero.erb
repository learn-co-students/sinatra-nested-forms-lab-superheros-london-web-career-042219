<h1>Create a Team and Heroes!</h1>
<form action="/team" method="post">
    <div>
        <label>Team Name:</label>
        <input type="text" name="team[name]">
    </div>
    <div>
        <label>Team Motto:</label>
        <input type="text" name="team[motto]">    
    </div>
    <h2>Hero 1 </h2>
    <div>
        <label>Hero's Name:</label> 
        <input type="text" name="team[members][][name]" id="member1_name"/>
    </div>
    <div>
        <label>Hero's Power:</label>
        <input type="text" name="team[members][][power]" id="member1_power"/>
    </div>
    <div>
        <label>Hero's Biography:</label>
        <input type="text" name="team[members][][biography]" id="member1_bio"/>
    </div>
     <h2>Hero 2 </h2>
    <div>
        <label>Hero's Name:</label> 
        <input type="text" name="team[members][][name]" id="member2_name"/>
    </div>
    <div>
        <label>Hero's Power:</label>
        <input type="text" name="team[members][][power]" id="member2_power"/>
    </div>
    <div>
        <label>Hero's Biography:</label>
        <input type="text" name="team[members][][biography]" id="member2_bio"/>
    </div>
     <h2>Hero 3 </h2>
    <div>
        <label>Hero's Name:</label> 
        <input type="text" name="team[members][][name]" id="member3_name"/>
    </div>
    <div>
        <label>Hero's Power:</label>
        <input type="text" name="team[members][][power]" id="member3_power"/>
    </div>
    <div>
        <label>Hero's Biography:</label>
        <input type="text" name="team[members][][biography]" id="member3_bio"/>
    </div>
    <input type="submit" value="submit"/>

</form>
